{
  "name": "ssrmicroservice",
  "version": "1.0.0",
  "description": "Microservice for SSR cloud rendering",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all --parallel watch server",
    "server": "nodemon server.js",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavel-one/SsrMicroservice.git"
  },
  "keywords": [
    "SSR"
  ],
  "author": "Pavel Zarubin <team@pavel.one>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pavel-one/SsrMicroservice/issues"
  },
  "homepage": "https://github.com/pavel-one/SsrMicroservice#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "buefy": "^0.9.4",
    "config": "^3.3.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "laravel-mix": "^5.0.9",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.8",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "resolve-url-loader": "^3.1.2",
    "session-file-store": "^1.5.0",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  }
}
